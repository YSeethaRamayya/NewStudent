<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.newstudent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/student"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Transportation"
            android:theme="@style/TransTheme"/>
        <activity android:name=".Academics"
            android:theme="@style/AcadTheme"/>
        <activity android:name=".Insurance"
            android:theme="@style/InsurTheme"/>
        <activity android:name=".Travelling"
            android:theme="@style/TravelTheme"/>
        <activity android:name=".CostofLiving"
            android:theme="@style/COLTheme"></activity>
    </application>

</manifest>